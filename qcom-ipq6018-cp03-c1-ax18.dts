#include "qcom-ipq6018.dtsi"
#include "qcom-ipq6018-rpm-regulator.dtsi"
#include "qcom-ipq6018-cpr-regulator.dtsi"
#include "qcom-ipq6018-cp-cpu.dtsi"
#include "dt-bindings/input/input.h"
#include "dt-bindings/gpio/gpio.h"
/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "AX18/M2/WF-1 IPQ6018/BSE-1GB";
	compatible = "qcom,ipq6018-cp03", "qcom,ipq6018";
	interrupt-parent = <&intc>;
	qcom,msm-id = <0x192 0x0>;
                    
	aliases {
		led-boot = "/soc/leds/led@58";
		led-failsafe = "/soc/leds/led@58";
		led-running = "/soc/leds/led@58";
		led-upgrade = "/soc/leds/led@58";
		serial0 = &blsp1_uart3;

		ethernet0 = "/soc/dp1";
		ethernet1 = "/soc/dp2";
		ethernet2 = "/soc/dp3";
		ethernet3 = "/soc/dp4";
	};

	chosen {
		bootargs = "console=ttyMSM0,115200,n8 rw init=/init";
		bootargs-append = " root=/dev/ubiblock0_1 swiotlb=1";
	};


	memory {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x20000000>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		nss@40000000 {
			no-map;
			reg = <0x0 0x40000000 0x0 0x01000000>;
		};

		uboot@4A100000 {
			no-map;
			reg = <0x0 0x4A100000 0x0 0x00400000>;
		};

		sbl@4A500000 {
			no-map;
			reg = <0x0 0x4A500000 0x0 0x00100000>;
		};

		tz@4A600000 {
			no-map;
			reg = <0x0 0x4A600000 0x0 0x00400000>;
		};

		smem_region:smem@4AA00000 {
			no-map;
			reg = <0x0 0x4AA00000 0x0 0x00100000>;
		};

		q6_region: wcnss@4ab00000 {
			no-map;
			reg = <0x0 0x4ab00000 0x0 0x03700000>;
		};

		q6_etr_region: q6_etr_dump@1 {
			no-map;
			reg = <0x0 0x4E200000 0x0 0x100000>;
		};

		m3_dump@4e300000 {
			no-map;
			reg = <0x0 0x4e300000 0x0 0x100000>;
		};

		rpm_msg_ram: rpm_msg_ram@0x60000 {
			no-map;
			reg = <0x0 0x60000 0x0 0x6000>;
		};
	};

	soc {
		gpio_keys {
			compatible = "gpio-keys";
			pinctrl-0 = <&button_pins>;
			pinctrl-names = "default";

			wps {
				label = "wps";
				linux,code = <KEY_WPS_BUTTON>;
				gpios = <&tlmm 60 GPIO_ACTIVE_LOW>;
				linux,input-type = <1>;
				debounce-interval = <60>;
			};

			mesh {
				label = "mesh";
				linux,code = <KEY_ATTENDANT_ON>;
				gpios = <&tlmm 9 GPIO_ACTIVE_LOW>;
				linux,input-type = <1>;
				debounce-interval = <60>;
			};
		};

		leds {
			compatible = "gpio-leds";
			pinctrl-0 = <&led_pins>;
			pinctrl-names = "default";

			led@73 {
				label = "led_2g";
				gpio = <&tlmm 73 GPIO_ACTIVE_HIGH>;
				default-state = "on";
			};

			led@74 {
				label = "wan_1";
				gpio = <&tlmm 74 GPIO_ACTIVE_HIGH>;
				default-state = "off";
			};

			led@69 {
				label = "wan_2";
				gpios = <&tlmm 69 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "wan_2";
				default-state = "off";
			};

			led@37 {
				label = "lan1";
				gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "lan1";
				default-state = "off";
			};

			led@35 {
				label = "lan2";
				gpios = <&tlmm 35 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "lan2";
				default-state = "off";
			};

			led@70 {
				label = "lan3";
				gpios = <&tlmm 70 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "lan3";
				default-state = "off";
			};

			led@58 {
				label = "power";
				gpios = <&tlmm 58 GPIO_ACTIVE_HIGH>;
				linux,default-trigger = "power";
				default-state = "on";
			};
		};

		mdio: mdio@90000 {
			pinctrl-0 = <&mdio_pins>;
			pinctrl-names = "default";
			phy-reset-gpio = <&tlmm 74 0>;
			status = "ok";

			phy0: ethernet-phy@0 {
				reg = <4>;
			};
			phy1: ethernet-phy@1 {
				reg = <0>;
			};
			phy2: ethernet-phy@2 {
				reg = <1>;
			};
			phy3: ethernet-phy@3 {
				reg = <3>;
			};
		};

		ess-switch@3a000000 {
			switch_cpu_bmp = <0x1>;  /* cpu port bitmap */
			switch_lan_bmp = <0x1e>; /* lan port bitmap */
			switch_wan_bmp = <0x20>; /* wan port bitmap */
			switch_inner_bmp = <0xc0>; /*inner port bitmap*/
			switch_mac_mode = <0x0>; /* mac mode for uniphy 0*/
			switch_mac_mode1 = <0xff>; /* mac mode for uniphy 1*/
			switch_mac_mode2 = <0xff>; /* mac mode for uniphy 2*/

			qcom,port_phyinfo {
				port@3 {
					port_id = <5>;
					phy_address = <4>;
				};
				port@0 {
					port_id = <1>;
					phy_address = <0>;
				};
				port@1 {
					port_id = <2>;
					phy_address = <1>;
				};
				port@2 {
					port_id = <4>;
					phy_address = <3>;
				};
			};
		};

		dp1 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <5>;
			reg = <0x3a001800 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <4>;
			phy-mode = "sgmii";
		};

		dp2 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <1>;
			reg = <0x3a001000 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <0>;
			phy-mode = "sgmii";
		};

		dp3 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <2>;
			reg = <0x3a001200 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <1>;
			phy-mode = "sgmii";
		};

		dp4 {
			device_type = "network";
			compatible = "qcom,nss-dp";
			qcom,id = <4>;
			reg = <0x3a001600 0x200>;
			qcom,mactype = <0>;
			local-mac-address = [000000000000];
			qcom,link-poll = <1>;
			qcom,phy-mdio-addr = <3>;
			phy-mode = "sgmii";
		};

		nss-macsec0 {
			compatible = "qcom,nss-macsec";
			phy_addr = <0x18>;
			phy_access_mode = <0x00>;
			mdiobus = <0x47>;
		};
	};
};

&tlmm {
	uart_pins: uart_pins {
		mux {
			pins = "gpio44", "gpio45";
			function = "blsp2_uart";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	spi_0_pins: spi_0_pins {
		mux {
			pins = "gpio38", "gpio39", "gpio40", "gpio41";
			function = "blsp0_spi";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	qpic_pins: qpic_pins {
		data_0 {
			pins = "gpio15";
			function = "qpic_pad0";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_1 {
			pins = "gpio12";
			function = "qpic_pad1";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_2 {
			pins = "gpio13";
			function = "qpic_pad2";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_3 {
			pins = "gpio14";
			function = "qpic_pad3";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_4 {
			pins = "gpio5";
			function = "qpic_pad4";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_5 {
			pins = "gpio6";
			function = "qpic_pad5";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_6 {
			pins = "gpio7";
			function = "qpic_pad6";
			drive-strength = <8>;
			bias-pull-down;
		};
		data_7 {
			pins = "gpio8";
			function = "qpic_pad7";
			drive-strength = <8>;
			bias-pull-down;
		};
		qpic_pad {
			pins = "gpio1", "gpio3", "gpio4",
			       "gpio10", "gpio11", "gpio17";
			function = "qpic_pad";
			drive-strength = <8>;
			bias-pull-down;
		};
	};

	button_pins: button_pins {
		wps_button {
			pins = "gpio60";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};

		mesh_button {
			pins = "gpio9";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	mdio_pins: mdio_pinmux {
		mux_0 {
			pins = "gpio64";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mux_1 {
			pins = "gpio65";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};

		mux_2 {
			pins = "gpio75";
			function = "gpio";
			bias-pull-up;
		};

		mux_3 {
			pins = "gpio77";
			function = "gpio";
			bias-pull-up;
		};
	};

	led_pins: led_pins {
		led_2g {
			pins = "gpio73";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-up;
		};

		power {
			pins = "gpio58";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-up;
		};

		wan_1 {
			pins = "gpio74";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-down;
		};

		wan_2 {
			pins = "gpio69";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-down;
		};

		lan1 {
			pins = "gpio37";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-down;
		};

		lan2 {
			pins = "gpio35";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-down;
		};

		lan3 {
			pins = "gpio70";
			function = "gpio";
			drive-strength = <0x08>;
			bias-pull-down;
		};
	};
};

&blsp1_uart3 {
	pinctrl-0 = <&uart_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&spi_0 {
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	cs-select = <0>;
	status = "ok";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "n25q128a11";
		spi-max-frequency = <50000000>;
		use-default-sizes;
	};
};

&nand {
	pinctrl-0 = <&qpic_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&nss_crypto {
	status = "ok";
};

&qpic_bam {
	status = "ok";
};

&qusb_phy_0 {
	status = "ok";
};

&ssphy_0 {
	status = "ok";
};

&dbm_1p5 {
	compatible = "qcom,usb-dbm-1p5";
	reg = <0x8AF8000 0x300>;
	qcom,reset-ep-after-lpm-resume;
		};

&usb3 {
	status = "ok";
};
